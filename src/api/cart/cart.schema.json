{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Cart",
    "description": "The Cart",
    "required": [
        "quantity",
        "billing_id",
        "item_id"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of a cart."
        },
        "billing_id": {
            "$id": "#/properties/billing_id",
            "type": "integer",
            "title": "Details of the bill."
        },
        "item_id": {
            "$id": "#/properties/item_id",
            "type": "integer",
            "title": "Id of the item."
        },
        "quantity": {
            "$id": "#/properties/quantity",
            "type": "integer",
            "title": "Quantity of the item bought."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the bill."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the bill was last updated."
        }
    }
  }