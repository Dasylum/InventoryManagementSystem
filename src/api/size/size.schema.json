{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Size",
    "description": "The sizes",
    "required": [
        "name",
        "length",
        "width",
        "shape_id"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of size."
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of the size."
        },
        "length": {
            "$id": "#/properties/length",
            "type": "number",
            "title": "Length of the item."
        },
        "width": {
            "$id": "#/properties/width",
            "type": "number",
            "title": "width of the item."
        },
        "height": {
            "$id": "#/properties/height",
            "type": "number",
            "title": "height of the item."
        },
        "radius": {
            "$id": "#/properties/radius",
            "type": "number",
            "title": "radius of the item."
        },
        "volume": {
            "$id": "#/properties/volume",
            "type": "number",
            "title": "volume of the item."
        },
        "shape_id": {
            "$id": "#/properties/shape_id",
            "type": "number",
            "title": "shape_id of the item."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the user."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the user was last updated."
        }
    }
  }