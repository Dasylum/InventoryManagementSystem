{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Address",
    "description": "The Address",
    "required": [
        "street_address_1",
        "city",
        "zipcode",
        "latitude",
        "longitude"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of address."
        },
        "street_address_1": {
            "$id": "#/properties/street_address_1",
            "type": "string",
            "title": " Street address 1"
        },
        "street_address_2": {
            "$id": "#/properties/street_address_2",
            "type": "string",
            "title": " Street address 2"
        },
        "city": {
            "$id": "#/properties/city",
            "type": "string",
            "title": "The city.",
            "examples": [
                "CJ"
            ]
        },
        "zipcode": {
            "$id": "#/properties/zipcode",
            "type": "string",
            "title": "The zipcode."
        },
        "latitude": {
            "$id": "#/properties/latitude",
            "type": "number",
            "title": "The latitude."
        },
        "longitude": {
            "$id": "#/properties/longitude",
            "type": "number",
            "title": "The longitude."
        },
        "password": {
          "$id": "#/properties/password",
          "type": "string",
          "title": "The users hashed password."
        },
        "state": {
            "$id": "#/properties/state",
            "type": "number",
            "title": "The state."
        },
        "country": {
            "$id": "#/properties/zipcode",
            "type": "number",
            "title": "The zipcode."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the user."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the user was last updated."
        }
    }
  }